{
  "manifest_version": 3,
  "name": "Phoenix Blocker",
  "description": "Browser extension for the Phoenix app.",
  "version": "1.0.0",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
    "action": {
      "default_icon": "images/phoenix.svg"
    },
    "web_accessible_resources": [
      "images/*.*",
      "fonts/*.woff",
      "scripts/*.*",
      "df_youtube/scripts/*.*",
      "df_youtube/css/*.*",
      "*.html"
    ],
    "background": {
      "service_worker": "scripts/phoenix.js",
      "type": "module"
    },
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["df_youtube/scripts/df_youtube.js"]
      }
    ],
  "permissions": ["<all_urls>","tabs", "nativeMessaging", "webNavigation"]
}